<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To
    inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
        <clear />
    <add key="custom-artifacts" value="artifacts/nuget" />
    <add
        key="nuget"
        value="https://api.nuget.org/v3/index.json" />
    <add
        key="local"
        value="./pkg" />
  </packageSources>
</configuration>